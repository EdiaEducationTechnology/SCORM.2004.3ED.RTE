  <name>sakai-scorm-tool</name>
  <groupId>org.sakaiproject</groupId>
  <artifactId>sakai-scorm-tool</artifactId>
  <description>
          This project creates a war containing the Sakai Scorm Tool
        </description>
  <packaging>war</packaging>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <resources>
      <resource>
        <directory>${basedir}/src/java</directory>
        <includes>
          <include>**/*.html</include>
          <include>**/*.js</include>
          <include>**/*.properties</include>
          <include>**/*.gif</include>
          <include>**/*.css</include>
          <include>**/*.png</include>
        </includes>
      </resource>
    </resources>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.sakaiproject.kernel</groupId>
      <artifactId>sakai-kernel-api</artifactId>
