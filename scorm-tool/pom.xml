    </dependency>
    <!-- SCORM dependencies -->
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-scorm-api</artifactId>
      <version>${sakai.version}</version>
      <type>jar</type>
      <scope>provided</scope>
    </dependency>
