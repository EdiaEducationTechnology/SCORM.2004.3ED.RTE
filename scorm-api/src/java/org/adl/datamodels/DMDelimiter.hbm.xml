<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="all" default-access="field" package="org.adl.datamodels" default-lazy="false">
	
	<class name="org.adl.datamodels.DMDelimiter" table="SCORM_DELIMITER_T">
	
		<id name="id" column="DELIM_ID" type="long">
            <generator class="native">
                <param name="sequence">SCORM_UID_S</param>
            </generator>
        </id>
        
        <one-to-one name="mDescription" cascade="none" class="org.adl.datamodels.DMDelimiterDescriptor"  constrained="true"/>
        
        <!-- JLR: Stripped these out from mDescription below 1/28 unique-key="DELIM_ID" index="SCORM_DELIM_DESC_IDX" -->
        
        <!--many-to-one name="mDescription" column="DESCRIPTION" class="org.adl.datamodels.DMDelimiterDescriptor" /-->	
        
		<property name="mValue" column="VALUE" type="string" />
	
	</class>
	
</hibernate-mapping>